					<script type="text/javascript" charset="utf-8">
					//<![CDATA[
						
						var contact_defaults = new Array();
						contact_defaults['fullname'] = 'Name';
						contact_defaults['email'] = 'Email';
						contact_defaults['organization'] = 'Organization';
						contact_defaults['message'] = 'Message';

						jQuery(function(){

							jQuery('form.contact').each(function(e){
								jQuery(this)
									.find('input.text, textarea')
									.each(function(ee){
										contactBlur(jQuery(this))
									})
									.unbind('focus')
									.bind('focus', function(){
										contactFocus(jQuery(this));
									})
									.unbind('blur')
									.bind('blur', function(){
										contactBlur(jQuery(this));
									});
								jQuery(this)
									.find('input.submit')
									.unbind('mouseenter')
									.bind('mouseenter', function(){
										jQuery(this).addClass('hover');
									})
									.unbind('mouseleave')
									.bind('mouseleave', function(){
										jQuery(this).removeClass('hover');
									});
							});

						});

						function contactFocus(input){
							var name = input.attr('name');
							var value = input.val();
							if( value == contact_defaults[name] ){ input.val(''); }
							if( !input.hasClass('focus') ){ input.addClass('focus'); }
						}

						function contactBlur(input){
							var name = input.attr('name');
							var value = input.val();
							if( value == '' ){ input.val(contact_defaults[name]); }
							if( input.hasClass('focus') ){ input.removeClass('focus'); }
						}
						
					//]]>
